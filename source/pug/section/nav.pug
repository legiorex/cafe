nav.nav
    - var items = ['Сладости', 'Доставка', 'Компания', 'Команда']
    - var dataItems = ['sweet', 'delivery', 'company', 'team']
    each item, key in items
        if item === 'Сладости'
            a.nav__link.nav__link--active(href="#")= item
        else
            a.nav__link(href="#")= item